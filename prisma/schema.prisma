// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "darwin","linux-musl-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model iad {
  id                            String    @id @default(uuid())
  account                       String?
  sync                          String?
  name                          String?
  email                         String
  statecountconnected           String?
  notifyFrom                    String?
  syncFrom                      String?
  oauth2                        Json?   
  imapServerInfo                Json?
  state                         String?
  stateCountAuthenticationError String?
  stateCountConnectError        String?
  lastErrorState                Json?     @default("{}")
  statecountsyncing             String?
  listRegistry                  String?
  copy                          String?
  tz                            String?
  smtp                          Json?     @default("{}")
  imap                          Json?     @default("{}")
  proxy                         String?
  smtpEhloName                  String?
  path                          String?
  webhooks                      String?
  webhookErrorFlag              String?    
}